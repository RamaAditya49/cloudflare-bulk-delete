{
  "name": "cloudflare-bulk-delete",
  "version": "1.2.0",
  "description": "Fix Cloudflare 'too many deployments to be deleted' error. Bulk delete Pages and Workers deployments safely.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "cf-bulk-delete": "./bin/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "cli": "node bin/cli.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "test:ci": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --ci --coverage --watchAll=false",
    "test:publish": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --ci --coverage --watchAll=false",
    "lint": "eslint src/ bin/ --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "build": "echo 'No build step required for Node.js project'",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run test:publish && npm run lint"
  },
  "keywords": [
    "cloudflare",
    "too many deployments to be deleted",
    "cloudflare pages bulk delete",
    "cloudflare workers bulk delete",
    "cloudflare deployment limit",
    "pages",
    "workers",
    "bulk-delete",
    "deployments",
    "deployment cleanup",
    "cli",
    "automation",
    "cleanup",
    "tool"
  ],
  "author": {
    "name": "Rama Aditya",
    "email": "ramaaditya49@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RamaAditya49/cloudflare-bulk-delete.git"
  },
  "bugs": {
    "url": "https://github.com/RamaAditya49/cloudflare-bulk-delete/issues"
  },
  "homepage": "https://github.com/RamaAditya49/cloudflare-bulk-delete#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "p-limit": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@types/jest": "^29.5.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "semantic-release": "^24.2.0"
  },
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ]
